
{namespace vv}


/**
 * Dashboard page.
 @param plugins Plugins list.
 */
{template .dashboard}
  <div class="dashboard-grid">
    {foreach $plugin in $plugins}
      <div class="pull-left">
        <a href="{$plugin.dashboard.url}">
          <i class="fa {$plugin.dashboard.icon}"></i>

          {$plugin.dashboard.name}
        </a>
      </div>
    {/foreach}
  </div>
{/template}

