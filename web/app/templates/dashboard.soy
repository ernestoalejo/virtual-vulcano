
{namespace vv}


/**
 * Dashboard page.
 @param plugins Plugins list.
 */
{template .dashboard}
  <div class="dashboard-grid">
    {foreach $plugin in $plugins}
      <div class="pull-left">
        <a href="{$plugin.url}">
          <img class="word-space" src="{$plugin.icon}" height="70" width="70">
        </a>
        <a href="{$plugin.url}">{$plugin.name}</a>
      </div>
    {/foreach}
  </div>
{/template}

